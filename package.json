{
  "name": "stream-markdown-vue-tailwind4",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "perf:run": "node scripts/perf-runner.mjs",
    "test": "vitest",
    "bump": "bumpp packages/stream-markdown/package.json --commit --push --tag && pnpm --filter stream-markdown publish --access public --no-git-checks"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@iconify-json/carbon": "^1.2.15",
    "@iconify/tailwind": "^1.2.0",
    "@vueuse/core": "^9.13.0",
    "shiki": "^3.19.0",
    "stream-markdown": "workspace:*",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^22.19.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.22",
    "bumpp": "^9.11.1",
    "eslint": "^9.39.1",
    "jsdom": "^19.0.0",
    "playwright": "^1.58.1",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^7.2.6",
    "vite-plugin-pages": "^0.33.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "stackblitz": {
    "startCommand": "pnpm --filter='./playground' run dev"
  }
}
